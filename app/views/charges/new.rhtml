<h1>New Charge</h1>

<% form_tag({ :action => 'create' }, :multipart => true) do %>
  <%= render :partial => 'form' %>

	<div class="links_bar">
		<div class='buttons'>
			<%= form_button('Create', 'submit', 'tick') %>		
			<%= form_button('Undo', 'reset', '') %>
			<%= link_to 'Back', :action => 'list' %>
		</div>
	</div>
<% end %>