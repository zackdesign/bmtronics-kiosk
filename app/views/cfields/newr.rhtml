<h1>Add Row to Plan Charge</h1>

<% form_tag( :action => 'creater' ) do %>
  <%= render :partial => 'form' %>

	<div class="links_bar">
		<div class='buttons'>
			<%= form_button('Create', 'submit', 'tick') %>
			<%= form_button('Undo', 'reset', 'undo') %>
			<%= link_to 'Back', :action => 'list' %>
		</div>
	</div>
<% end %>