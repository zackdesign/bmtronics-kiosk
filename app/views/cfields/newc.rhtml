<h1>Add Column to Plan Charge Row</h1>

<% form_tag( :action => 'createc' ) do %>
  <%= render :partial => 'form_column' %>

	<div class="links_bar">
		<div class='buttons'>
			<%= form_button('Create', 'submit', 'tick.png') %>
			<%= form_button('Undo', 'reset', 'undo.png') %>
			<%= link_to 'Back', :action => 'modify', :id => @charge_id %>
		</div>
	</div>
<% end %>
