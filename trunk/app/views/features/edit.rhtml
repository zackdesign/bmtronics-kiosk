<h1>Edit Feature</h1>

<% form_tag({ :action => 'update', :id => @feature }, :multipart => true) do %>
  <%= render :partial => 'form' %>

	<div class="links_bar">
	  <div class='buttons'>
			<%= form_button('Update') %>
			<%= form_button('Undo', 'reset', 'undo.png') %>
			<%= link_to 'Show', :action => 'show', :id => @feature %>
			<%= link_to 'Back', :action => 'list' %>
		</div>
	</div>

<% end %>
